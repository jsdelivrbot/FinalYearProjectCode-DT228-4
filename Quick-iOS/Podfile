    pod 'Alamofire', '~> 4.0' # Swift 3: True
    pod 'SwiftyJSON'  # Swift 3: True
    pod 'JWTDecode', '~> 2.0' # Swift 3: True
    pod 'Cartography', :git => 'https://github.com/robb/Cartography', :branch => 'orta-swift3'
